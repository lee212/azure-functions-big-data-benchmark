{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "blob",
      "name": "inputBlob",
      "path": "pavlo/text/1node/rankings/{key}",
      "connection": "bigdata0benchmark_STORAGE",
      "direction": "in"
    },
    {
      "type": "blob",
      "name": "outputBlob",
      "path": "scan/{output}",
      "connection": "bigdata0benchmark_STORAGE",
      "direction": "out"
    }

  ],
  "disabled": false
}
